{
  "name": "express-react-redux-boilerplate",
  "version": "1.0.0",
  "description": "Express + React + Redux + Weppack 4 + babel 7 + hot reload server + hot reload client",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development babel-node ./src/server.js",
    "debug": "NODE_ENV=development babel-node --inspect ./src/server.js",
    "server": "NODE_ENV=production babel-node ./src/server.js",
    "build": "NODE_ENV=production ./node_modules/.bin/babel-node ./node_modules/.bin/webpack --mode production",
    "build-dev": "NODE_ENV=development ./node_modules/.bin/babel-node ./node_modules/.bin/webpack --mode development",
    "test": "jest",
    "storybook": "start-storybook -p 3002",
    "build-storybook": "build-storybook"
  },
  "author": "gissehel",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.2.1",
    "express": "^4.16.4",
    "fs-extra": "^7.0.0",
    "history": "^4.7.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
